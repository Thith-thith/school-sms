    //   "@api/*": ["api/*"],
    //   "@pages/*": ["pages/*"],
    //   "@public/*": ["public/*"],
    //   "@hooks/*": ["hooks/*"],
    //   "@utils/*": ["utils/*"]
    // }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
